<div id="work" class="page">
  <div class="container">
      <!-- Title Page -->
        <div class="row">
            <div class="span12">
                <div class="title-page">
                    <h2 class="title"><%=@project.name%></h2>
                    <h3 class="title-description"> <a href="#"><%=@project.description%></a> at <%=@project.address%></h3>
                    <div> 
                        <%=image_tag(@project.image, style: "height:300px; width:450px;")%>

                        <%= image_tag "http://maps.google.com/maps/api/staticmap?size=450x300&sensor=false&zoom=16&markers=#{@project.latitude}%2C#{@project.longitude}" %>

                    </div>
                    <div class="comments">
                      <div class="commentheader"> <strong>Comments</strong> </div>
                        <div>
                          <%=form_tag "/users/#{current_user.id}/projects/#{@project.id}/comments", :method => "post", :remote => true do %>
                          <%=label_tag "Enter Comment" %>
                          <%=text_area_tag :content, nil, class: "commentarea" %> <br>
                          <%=submit_tag "Submit Comment", class: "sc" %>
                          <% end %>
                        </div> 
                        <div class="commentplace">
                            <%@comments.each do |comment| %>
                            <h5><%=comment.user.name%></h5>
                            <p><%=comment.content%></p>
                            <% end %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Title Page -->
        
